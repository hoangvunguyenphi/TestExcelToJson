    <script>
        $(function () {
            //Initialize Select2 Elements
            $('.select2').select2()

            //Timepicker
            $('.timepicker').timepicker({
                showInputs: true,
                showMeridian: false
            })

            //Date range picker
            $('.reservation').daterangepicker(
                {
                    locale: {
                        format: 'DD/MM/YYYY'
                    }
                }
            )

            //Date picker
            $('.datepicker').datepicker({
                autoclose: true,
                format: 'dd/mm/yyyy'
            });

            $(".datepicker").datepicker('setDate', new Date());

            $(".btn-edit").click(function () {
                $('.btn-edit').hide();
                $(".span-value-show").hide();
                $(".input-hide").show();
                $('.btn-save').show();
                $('.btn-cancel').show();
            });

            $(".btn-cancel").click(function () {
                $('.btn-save').hide();
                $('.btn-cancel').hide();
                $('.btn-edit').show();
                $(".span-value-show").show();
                $(".input-hide").hide();

            });

            $(".nav-tabs a").click(function () {
                $('.btn-save').hide();
                $('.btn-cancel').hide();
                $('.btn-edit').show();
                $(".span-value-show").show();
                $(".input-hide").hide();
            })

        });

    </script>
